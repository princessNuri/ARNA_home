<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.main.cart.CartFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent">


        <ImageView
            android:id="@+id/arrow_back"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/baseline_arrow_back_24"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_MyCart"
            android:layout_width="72dp"
            android:layout_height="28dp"
            android:layout_marginStart="150dp"
            android:layout_marginEnd="140dp"
            android:fontFamily="@font/poppins"
            android:text="My Cart    "
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="@color/black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.utils.widget.ImageFilterView
        android:id="@+id/img_minstand"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/some_id"
        android:layout_marginBottom="500dp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_minstand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Minimal Stand"
        android:textColor="@color/minimal_sts"
        android:textSize="14sp"
        android:fontFamily="@font/nunito_sans"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.451"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="66dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/img_coffee"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/some_id"
        android:layout_marginBottom="250dp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/tv_coffee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Coffee Table"
        android:textColor="@color/minimal_sts"
        android:textSize="14sp"
        android:fontFamily="@font/nunito_sans"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="191dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/img_mindesk"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/some_id"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_mindesk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Minimal Desk"
        android:textSize="14sp"
        android:textColor="@color/minimal_sts"
        android:fontFamily="@font/nunito_sans"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="316dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>